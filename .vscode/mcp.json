{
  "servers": {
    "manas": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-filesystem",
          "${workspaceFolder}/Manas"
        ]
    },
    "github-mcp-server": {
      "url": "https://api.githubcopilot.com/mcp/",
      "type": "http"
    },
    "asana": {
    "command": "npx",
    "args": ["mcp-remote", "https://mcp.asana.com/sse"]
    }
  }
}